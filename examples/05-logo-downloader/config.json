{
  "data": {
    "format": "csv"
  },
  "globals": {
    "concurrency": 2,
    "tmpDir": "out/05-logo-downloader/.tmp"
  },
  "steps": [
    {
      "plugins": [
        {
          "type": "logo-scraper",
          "url": "{{website_url}}",
          "maxCandidates": 10,
          "minScore": 6,
          "logoPath": "out/05-logo-downloader/logos/{{website_url}}.png",
          "faviconPath": "out/05-logo-downloader/favicons/{{website_url}}.png",
          "output": {
            "mode": "merge"
          }
        }
      ],
      "output": {
        "mode": "ignore"
      }
    }
  ]
}
